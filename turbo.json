		"dependsOn": ["^build"],
		"outputs": [
		  "themes/enfantterrible-block-theme/dist/**",
		  "mu-plugins/enfantterrible-plugin/dist/**"
		]
	  },
	  "start": {
		"dependsOn": ["^build"],
